{
    "name": "Quizlet Helper",
    "version": "1.0.0",
    "description": "Makes it possible to have multiple saved lists in quizlet",
    "manifest_version": 3,
    "author": "Theo Karlsson",
    "permissions": [
        "activeTab",
        "storage",
        "tabs"
    ],
    "action": {
        "default_popup": "index.html",
        "default_title": "Hi"
    },
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["script.js"]
        }
    ]
}